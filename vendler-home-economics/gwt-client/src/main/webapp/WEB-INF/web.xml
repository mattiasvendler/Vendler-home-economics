<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>se.vendler.gwtclient.Entries EntriesController</servlet-name>
        <servlet-class>se.vendler.gwtclient.server.EntriesControllerImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>se.vendler.gwtclient.Entries AccountController</servlet-name>
        <servlet-class>se.vendler.gwtclient.server.AccountControllerImpl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>se.vendler.gwtclient.Entries EntriesController</servlet-name>
        <url-pattern>/EntriesController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>se.vendler.gwtclient.Entries AccountController</servlet-name>
        <url-pattern>/AccountController</url-pattern>
    </servlet-mapping>

</web-app>
