<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>se.vendler.gwtclient.App AppService</servlet-name>
        <servlet-class>se.vendler.gwtclient.server.AppServiceImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>se.vendler.gwtclient.App TableService</servlet-name>
        <servlet-class>se.vendler.gwtclient.server.TableServiceImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>se.vendler.gwtclient.Entries EntriesService</servlet-name>
        <servlet-class>se.vendler.gwtclient.server.EntriesServiceImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>se.vendler.gwtclient.App MainService</servlet-name>
        <servlet-class>se.vendler.gwtclient.server.MainServiceImpl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>se.vendler.gwtclient.Entries EntriesService</servlet-name>
        <url-pattern>/EntriesService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>se.vendler.gwtclient.App AppService</servlet-name>
        <url-pattern>/AppService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>se.vendler.gwtclient.App TableService</servlet-name>
        <url-pattern>/TableService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>se.vendler.gwtclient.App MainService</servlet-name>
        <url-pattern>/sMainService</url-pattern>
    </servlet-mapping>
</web-app>
